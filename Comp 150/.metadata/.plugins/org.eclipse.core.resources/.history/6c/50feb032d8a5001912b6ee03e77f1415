
public class Generate {

	public static void main(String[] args) {
		int x = (Random.rand(0)- (Panel2D.WIDTH/2)) + (Random.rand(Panel2D.WIDTH/2));
		int y = (Random.rand(0)- (Panel2D.HEIGHT/2)) + (Random.rand(Panel2D.HEIGHT/2));
		for(int i = 0; i<=11; i++) { 
			int x = (Random.rand(0)- (Panel2D.WIDTH/2)) + (Random.rand(Panel2D.WIDTH/2));
			int y = (Random.rand(0)- (Panel2D.HEIGHT/2)) + (Random.rand(Panel2D.HEIGHT/2));
			System.out.println((x+" "+y));
		}
	}

}
